<?xml version='1.0' encoding='utf-8'?>
<widget id="com.defqon1.tracker" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DefQon.1: Location Tracker</name>
    <description>
        See where your friends are hanging out, in realtime.
    </description>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
</widget>
